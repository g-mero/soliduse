{
  "name": "solid-uses",
  "type": "module",
  "version": "0.10.0",
  "private": false,
  "description": "Try make a tiny vueuse port for SolidJS",
  "license": "MIT",
  "keywords": [
    "solid",
    "solidjs",
    "vueuse",
    "utils"
  ],
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {},
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsup --dts",
    "serve": "vite preview",
    "pub": "pnpm build && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit --skipLibCheck"
  },
  "peerDependencies": {
    "solid-js": ">=1.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.8.1",
    "@types/node": "^22.9.0",
    "@unocss/reset": "^66.0.0",
    "@unocss/vite": "^66.0.0",
    "esbuild-plugin-solid": "^0.6.0",
    "eslint": "^9.15.0",
    "eslint-plugin-solid": "^0.14.4",
    "tsup": "^8.4.0",
    "typescript": "^5.4.5",
    "unocss": "^66.0.0",
    "vite": "^6.2.1",
    "vite-plugin-solid": "^2.10.2"
  },
  "browser": {},
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
